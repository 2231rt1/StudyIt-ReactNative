{
  "expo": {
    "name": "StudyIt",
    "slug": "StudyIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./public/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./public/assets/splash.png"
    },
    "plugins": ["expo-localization"]
  }
}
